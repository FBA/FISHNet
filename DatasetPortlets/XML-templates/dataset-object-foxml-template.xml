  <foxml:datastream ID="POLICY" STATE="A" CONTROL_GROUP="X" VERSIONABLE="true">
    <foxml:datastreamVersion ID="POLICY.0" LABEL="inlineXACMLpolicy" CREATED="2011-02-04T16:01:41.399Z" MIMETYPE="text/xml" SIZE="0">
      <foxml:contentDigest TYPE="DISABLED" DIGEST="none"/>
      <foxml:xmlContent>
        <Policy RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:first-applicable" xmlns="urn:oasis:names:tc:xacml:1.0:policy" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
          <Description>(PID Message)</Description>
          <Target>
            <Subjects>
              <AnySubject/>
            </Subjects>
            <Resources>
              <Resource>
                <ResourceMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
                  <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">(PID)</AttributeValue>
                  <ResourceAttributeDesignator AttributeId="urn:fedora:names:fedora:2.1:resource:object:pid" DataType="http://www.w3.org/2001/XMLSchema#string" />
                </ResourceMatch>
              </Resource>
            </Resources>
            <Actions>
              <AnyAction/>
            </Actions>
          </Target>
          <Rule Effect="Permit" RuleId="1">
            <!-- ************************************************************************************************************************************************ -->
            <!-- Permission is conditional upon the user login Id being one of the users identified in the set below or the role being the fedora administrator.  -->
            <!-- ************************************************************************************************************************************************ -->
            <Target>
              <Subjects>
                <AnySubject/>
              </Subjects>
              <Resources>
                <AnyResource/>
              </Resources>
              <Actions>
                <AnyAction/>
              </Actions>
            </Target>
            <Condition FunctionId="urn:oasis:names:tc:xacml:1.0:function:string-at-least-one-member-of">
              <SubjectAttributeDesignator DataType="http://www.w3.org/2001/XMLSchema#string" AttributeId="fedoraRole"/>
              <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:string-bag">
                <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">administrator</AttributeValue>
              </Apply>
            </Condition>
          </Rule>
          <Rule Effect="Permit" RuleId="2">
            <!-- *********************************************************************************************** -->
            <!-- Permission is conditional upon the request being for the getBasicMetadata disseminator method.  -->
            <!-- *********************************************************************************************** -->
            <Target>
              <Subjects>
                <AnySubject/>
              </Subjects>
              <Resources>
                <Resource>
                  <ResourceMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
                    <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">DatasetConfig:XSLT-SDEF</AttributeValue>
                    <ResourceAttributeDesignator AttributeId="urn:fedora:names:fedora:2.1:resource:sdef:pid" DataType="http://www.w3.org/2001/XMLSchema#string"/>
                  </ResourceMatch>
                  
                  <ResourceMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
                    <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">getDocumentStyle1</AttributeValue>
                    <ResourceAttributeDesignator AttributeId="urn:fedora:names:fedora:2.1:resource:disseminator:method" DataType="http://www.w3.org/2001/XMLSchema#string"/>
                  </ResourceMatch>
                  <ResourceMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
                    <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">getDocumentStyle2</AttributeValue>
                    <ResourceAttributeDesignator AttributeId="urn:fedora:names:fedora:2.1:resource:disseminator:method" DataType="http://www.w3.org/2001/XMLSchema#string"/>
                  </ResourceMatch>
                  <!--
                  <Condition FunctionId="urn:oasis:names:tc:xacml:1.0:function:string-at-least-one-member-of">
                    <ResourceAttributeDesignator AttributeId="urn:fedora:names:fedora:2.1:resource:disseminator:method" DataType="http://www.w3.org/2001/XMLSchema#string"/>
                    <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:string-bag">
		      <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">getDocumentStyle1</AttributeValue>
		      <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">getDocumentStyle2</AttributeValue>
		    </Apply>
		  </Condition>
                  -->
                </Resource>                
              </Resources>
              <Actions>
                <Action>
                  <ActionMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
                    <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">urn:fedora:names:fedora:2.1:action:id-getDissemination</AttributeValue>
                    <ActionAttributeDesignator DataType="http://www.w3.org/2001/XMLSchema#string" AttributeId="urn:fedora:names:fedora:2.1:action:id"/>
                  </ActionMatch>
                </Action>
              </Actions>
            </Target>
          </Rule>
          <Rule Effect="Deny" RuleId="3">
            <!-- **************************************************** -->
            <!-- If none of the above then don't permit the request.  -->
            <!-- **************************************************** -->
          </Rule>
        </Policy>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
